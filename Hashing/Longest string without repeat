int Solution::lengthOfLongestSubstring(string A) {
    //"abcabcbb" is "abc", which the length is 3.
    int n= A.length();
    unordered_map<char,int > ump;
    int len=0,j=-1;
    for(int i=0;i<n;i++){
        if(ump.find(A[i])!=ump.end()){
            j=max(ump[A[i]],j);  
        }
        len=max(i-j,len);
        ump[A[i]]=i;
    }
    return len;
}
